{
  "name": "NeuraNet",
  "version": "1.0.0",
  "private": true,
  "description": "A decentralized cloud storage system.",
  "main": "./dist/main.js",
  "icon": "./icon.png",
  "homepage": "./dist/renderer/index.html",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.ts --mode=development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/react.webpack.ts --mode=development",
    "electron:serve": "wait-on http-get://localhost:8081/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.ts --mode=production",
    "build:react": "webpack --config webpack/react.webpack.ts --mode=production",
    "package": "npm-run-all build package:dist",
    "deploy": "npm-run-all build package:dist2",
    "package:dist": "electron-builder --dir",
    "package:dist2": "electron-builder"
  },
  "keywords": [],
  "author": "Banbury",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.9.1",
    "@mui/x-charts": "^6.19.4",
    "@mui/x-data-grid": "^6.19.4",
    "@mui/x-tree-view": "^6.17.0",
    "copy-webpack-plugin": "^12.0.2",
    "frameless-titlebar": "^2.1.4",
    "node-fetch": "^3.3.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.3.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "build": {
    "appId": "com.company.app",
    "productName": "NeuraNet",
    "directories": {
      "output": "packages",
      "buildResources": "resources"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "build/icon.png"
    },
    "mac": {
      "target": [
        "dmg",
        "pkg"
      ],
      "icon": "static/NeuraNet_Icons/macos/AppIcon.icns"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "appImage",
        "flatpak",
        "pacman",
        "rpm",
        "freebsd",
        "p5p",
        "apk"
      ],
      "icon": "static/NeuraNet_Icons/macos/AppIcon.icns"
    },
    "files": [
      "package.json",
      "dist/**",
      "dist/renderer/**"
    ],
 
    "extraResources": [
      {
        "from": "python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "venv",
        "to": "python",
        "filter": [
          "**/*"
        ]
      }
 
    ]
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.13.0"
  }
}




